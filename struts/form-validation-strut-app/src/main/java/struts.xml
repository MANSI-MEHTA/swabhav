<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="main" namespace="/" extends="json-default">
		<action name="" class="">
			<result name="success">/views/index.html</result>
		</action>

		<action name="form" class="com.techlabs.actions.Form" method="execute">
			<result name="success">/views/form.jsp</result>
		</action>
		<action name="showjson" class="com.techlabs.actions.Form"
			method="jsonExecute">
			<result type="json"></result>
		</action>

	</package>
</struts>