		this.conn=conn;
	}
	public String swabhav_getDate(){
		try{
		PreparedStatement stmt=conn.prepareStatement("SELECT getDate()");
		ResultSet rs=stmt.executeQuery();
		while(rs.next()){
			rs.getS
		}
		}catch(Exception ex){
			System.out.println(ex.getMessage());
		}
	}
}
