package com.techlabs.service;

import java.util.ArrayList;
import java.util.List;

import com.techlabs.model.Product;

public class ProductService {
	private List<Product> listofproducts = new ArrayList<Product>();
	private ProductService object;
	private ProductService(){
		
	}
	public ProductService getInstanceOf(){
		if(object==null){
			object=new ProductService();
			init();
		}
		return object;
	}
	public void init(){
		listofproducts.add(new Product("P101","laptop",50000,5000));
		listofproducts.add(new Product("P102","mobile",10000,1000));
	}
	
} 
