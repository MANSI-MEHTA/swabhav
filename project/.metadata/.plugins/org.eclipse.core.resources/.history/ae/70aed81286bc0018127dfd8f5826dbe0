package com.techlabs.entity;

import javax.persistence.Column;
import javax.persistence.Id;
import javax.persistence.OneToOne;

public class Account {

	@Id
	@Column(name="acc_no")
	private long acc_no;
	
	@Column(name="balance")
	private double balance;
	
	@OneToOne(mappedBy="account")
	private User user;
}
